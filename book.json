{
  "plugins": [
    "page-toc",
    "custom-header-next@git+https://github.com/dskvr/gitbook-plugin-custom-header-next.git",
    "favicon",
    "toggle-chapters",
    "versions"
  ],
  "pluginsConfig": {
    "page-toc": {
      "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
      "position": "before-first",
      "showByDefault": true
    },
    "custom-header-next": {
      "logo":"/eosio.cdt/images/logo.png",
      "logo_alt":"EOSIO",
      "base_path":"/eosio.cdt",
      "text": "EOSIO.CDT",
      "navigations": []
    },
    "favicon":{
        "shortcut": "images/favicon.ico",
        "bookmark": "images/favicon.ico"
    },
    "versions": {
      "gitbookConfigURL": "/book.json",
      "options": [
        {
          "value": "1.2.1/",
          "text": "Version v1.2.1"
        },
        {
          "value": "1.3.0/",
          "text": "Version v1.3.0"
        },
        {
          "value": "1.3.1/",
          "text": "Version v1.3.1"
        },
        {
          "value": "1.3.2/",
          "text": "Version v1.3.2"
        }
      ]
    }
  }
}
